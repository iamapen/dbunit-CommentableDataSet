{
  "name": "iamapen/excel-friendly-data-set",
  "type": "library",
  "description": "Excel friendly CsvDataSet for DBUnit",
  "homepage": "https://github.com/iamapen/dbunit-ExcelFriendlyDataSet",
  "keywords": ["phpunit", "dbunit"],
  "license": "MIT",
  
  "minimum-stability" : "stable",
  "authors": [
    {
      "name": "Yosuke Kushida",
      "email": "iamapen@studiopoppy.com"
    }
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://openpear.org"
    }
  ],
  
  "require": {
    "php": ">=5.3.1",
    "ext-pdo": "*",
    "pear-openpear.org/Stream_Filter_Mbstring": "= 0.0.7"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  
  "autoload": {
    "psr-0": {
      "Iamapen\\ExcelFriendlyDataSet\\ExcelCsvDataSet": "src/",
      "Iamapen\\ExcelFriendlyDataSet\\CommentableCsvDataSet": "src/"
    }
  },
  
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  }
}
