{
  "name": "iamapen/excel-friendly-data-set",
  "type": "library",
  "description": "Excel friendly CsvDataSet for DBUnit",
  "homepage": "https://github.com/iamapen/dbunit-ExcelFriendlyDataSet",
  "keywords": ["phpunit", "dbunit"],
  "license": "MIT",
 
  "minimum-stability" : "stable",
  "authors": [
    {
      "name": "Yosuke Kushida",
      "email": "iamapen@studiopoppy.com"
    }
  ],

  "require": {
    "php": ">=5.3.1",
    "ext-pdo": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "phpunit/dbunit": "~1.2"
  },

  "autoload": {
    "psr-0": {
      "Iamapen\\ExcelFriendlyDataSet\\ExcelCsvDataSet": "src/lib/",
      "Iamapen\\ExcelFriendlyDataSet\\CommentableCsvDataSet": "src/lib/"
    }
  },

  "extra": {
    "_branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}

