{
  "name": "iamapen/commentable-data-set",
  "type": "library",
  "description": "Commentable CSV DataSet for DBUnit",
  "homepage": "https://github.com/iamapen/dbunit-CommentableDataSet",
  "keywords": ["phpunit", "dbunit"],
  "license": "MIT",

  "minimum-stability" : "stable",
  "authors": [
    {
      "name": "Yosuke Kushida",
      "email": "iamapen@studiopoppy.com"
    }
  ],

  "require": {
    "php": ">=5.3.3",
    "ext-mbstring": "*",
    "phpunit/dbunit": "~1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },

  "autoload": {
    "psr-4": {
      "Iamapen\\CommentableDataSet\\": "src/lib/Iamapen/CommentableDataSet/",
      "Iamapen\\ExcelFriendlyDataSet\\": "src/lib/Iamapen/ExcelFriendlyDataSet/"
    }
  },

  "extra": {
    "_branch-alias": {
    }
  }
}
